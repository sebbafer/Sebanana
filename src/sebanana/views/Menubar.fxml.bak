<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="MenuBar" fx:id="menubar" xmlns:fx="http://javafx.com/fxml">
  <menus>
    <Menu mnemonicParsing="false" text="Bestand">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#newGame" text="New Game" fx:id="newgame" />
        <MenuItem mnemonicParsing="false" onAction="#open" text="Open" fx:id="open" />
        <MenuItem mnemonicParsing="false" onAction="#doSave" text="Save" fx:id="save" />
        <SeparatorMenuItem mnemonicParsing="false" />
        <MenuItem mnemonicParsing="false" onAction="#exit" text="Exit" fx:id="exit" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Bewerken">
      <items>
        <MenuItem mnemonicParsing="false" text="Instellingen" fx:id="toonInstellingen" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Hulp">
      <items>
        <Menu mnemonicParsing="false" text="Cheats">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#extraEnergy" text="Extra energy" fx:id="cheat1" />
            <MenuItem mnemonicParsing="false" onAction="#extraHappiness" text="Extra happiness" fx:id="cheat2" />
            <MenuItem mnemonicParsing="false" onAction="#addPoints" text="Extra points" fx:id="cheat3" />
          </items>
        </Menu>
        <MenuItem mnemonicParsing="false" onAction="#showAboutWindow" text="Over" fx:id="over" />
      </items>
    </Menu>
  </menus>
</fx:root>
