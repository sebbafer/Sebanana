<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<fx:root type="BorderPane" prefHeight="432.0" prefWidth="454.0" xmlns:fx="http://javafx.com/fxml" fx:controller="sebanana.views.GameviewController">
  <center>
    <VBox prefHeight="200.0" prefWidth="100.0">
      <children>
        <Label fx:id="content" alignment="TOP_LEFT" prefHeight="173.0" prefWidth="454.0" text="" wrapText="false" VBox.vgrow="ALWAYS">
          <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </VBox.margin>
        </Label>
        <HBox prefHeight="100.0" prefWidth="200.0">
          <children>
            <Label text="U bevindt zich in:" />
            <Label fx:id="roomLabel" text="" />
          </children>
        </HBox>
      </children>
    </VBox>
  </center>
  <top>
    <MenuBar>
      <menus>
        <Menu mnemonicParsing="false" text="Bestand">
          <items>
            <MenuItem mnemonicParsing="false" text="Afsluiten" onAction="#closeStage"  />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Bewerk" />
        <Menu mnemonicParsing="false" text="Hulp">
          <items>
            <MenuItem mnemonicParsing="false" text="Over" onAction="#showAboutWindow"  />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </top>
</fx:root>
